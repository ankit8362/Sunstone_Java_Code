//  Perfect Number find a divisor and add this if is equal, called perfect numbers
import java.util.*;
public class Main{
    public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt(); // 6
    int sum=0;
    for(int i=1;i<=n/2;i++){
        if(n%i==0){
            sum+=i;
        }
    }
    if(sum==n) System.out.println("Perfect Number");
    else System.out.println(n+" is not a perfect number");
    }
}
